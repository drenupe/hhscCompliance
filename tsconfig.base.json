        "libs/features/enclosed-beds/src/index.ts"
      ],
      "@hhsc-compliance/finance": ["libs/features/finance/src/index.ts"],
      "@hhsc-compliance/home": ["libs/features/home/src/index.ts"],
      "@hhsc-compliance/iss": ["libs/features/iss/src/index.ts"],
      "@hhsc-compliance/medical": ["libs/features/medical/src/index.ts"],
