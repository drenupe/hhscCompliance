  "private": true,
  "scripts": {
    "build": "nx build server --configuration=production",
    "build:server": "nx build server",
    "build:web": "nx build web",
    "dev:server": "nx serve server",
    "dev:web": "nx serve web",
    "deploy:server": "nx run server:prune",
    "start": "node dist/api/main.js",
    "start:dist": "node dist/api/main.js",
    "lint": "nx lint",
    "test": "nx test",
    "e2e:server": "nx run server-e2e:e2e",
    "e2e:web": "nx e2e web-e2e",
    "db:migrate": "cross-env DOTENV_CONFIG='{\"path\":\"server.env\"}' TS_NODE_PROJECT=server/tsconfig.typeorm.json node -r ts-node/register -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js -d server/src/configuration/typeorm/data-source.ts migration:run",
    "db:revert": "cross-env TS_NODE_PROJECT=server/tsconfig.typeorm.json typeorm-ts-node-commonjs -r tsconfig-paths/register -r dotenv/config -d server/src/configuration/typeorm/data-source.ts migration:revert",
    "db:gen:init_auth": "cross-env TS_NODE_PROJECT=server/tsconfig.typeorm.json typeorm-ts-node-commonjs -r tsconfig-paths/register -r dotenv/config -d server/src/configuration/typeorm/data-source.ts migration:generate server/migrations/init_auth",
    "typeorm": "cross-env TS_NODE_PROJECT=server/tsconfig.typeorm.json node ./node_modules/typeorm/cli.js -d server/src/configuration/typeorm/data-source.ts",
    "ssl:check": "cross-env TS_NODE_PROJECT=server/tsconfig.typeorm.json ts-node -r tsconfig-paths/register -r dotenv/config server/scripts/ssl-check.ts",
    "patch:project": "tsx web/tools/patch-project-json.ts",
    "patch:frontend-config": "tsx ./frontend/tools/update-project-json.ts"
  },
